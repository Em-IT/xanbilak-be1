generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model ShoppingItems {
  id          Int       @id @default(autoincrement())
  title       String
  count       Int?      @default(1)
  isBought    Boolean   @default(false)
  description String?
  createDate  DateTime  @default(now())
  updateDate  DateTime?
  buyDate     DateTime?
}
